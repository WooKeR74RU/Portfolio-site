                    <img src="public/myResource/myImage/my_photo.jpg" class="img-flex profile-photo">
                    <a>
                        <div class="mask"></div>
                    </a>
                </div>
                <div class="card-body text-center">
                    <h3 class="card-title"><strong>{{ res.myFullName }}</strong></h3>
                    <h4>{{ res.developer }}</h4>

                    <p class="card-text" style="font-size: 1.2em">{{ res.myDescribe }}
                    </p>
                </div>
                <div class="media mb-1 skill">
                    <div class="media-body">
                        <h4 class="">{{ res.technologyStack }}</h4>
                        <p>
                            <span class="badge grey badge-element" style="font-size: 100%">Git/Bitbacket</span>
                            <span class="badge blue badge-element" style="font-size: 100%">SQLite</span>
                            <span class="badge red badge-element" style="font-size: 100%">Firebase</span>
                            <span class="badge red badge-element" style="font-size: 100%">и т.д</span>

                            <!--
                        <h4 class="">{{ res.specialization }}</h4>
