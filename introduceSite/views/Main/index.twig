{% extends '../layout.twig' %}



{% block content %}


<div class="card card-cascade profile-card">

    <div class="profile-info">

        <div>
            <div class="view overlay hm-white-slight">
                <img src="public/myResource/myImage/my_photo.jpg" class="img-flex" style="margin-left: 50%; transform: translate(-50%,0);">
                <a>
                    <div class="mask"></div>
                </a>
            </div>
            <div class="card-body text-center">
                <h3 class="card-title"><strong>{{ res.myFullName }}</strong></h3>
                <h4>{{ res.developer }}</h4>

                <p class="card-text" style="font-size: 1.2em">{{ res.myDescribe }}
                </p>
            </div>
            <div class="media mb-1 skill">
                <div class="media-body">
                    <h4 class="">{{ res.technologyStack }}</h4>
                    <p>
                        <span class="badge blue badge-element" style="font-size: 100%">NodeJs</span>
                        <span class="badge blue badge-element" style="font-size: 100%">.Net</span>
                        <span class="badge blue badge-element" style="font-size: 100%">Twig</span>
                        <span class="badge blue badge-element" style="font-size: 100%">MongoDB</span>
                        <span class="badge white-text black badge-element" style="font-size: 100%">Unity</span>
                        <span class="badge green badge-element" style="font-size: 100%">Keras</span>
                        <span class="badge grey badge-element" style="font-size: 100%">Git</span>
                        <span class="badge white badge-element black-text" style="font-size: 100%">GraphQL</span>
                        <span class="badge orange badge-element" style="font-size: 100%">SQL databases</span>
                        <span class="badge blue badge-element" style="font-size: 100%">Django</span>

                    <h4 class="">{{ res.specialization }}</h4>
                    <span class="badge orange badge-element" style="font-size: 100%">Android development</span>
                    <span class="badge orange badge-element" style="font-size: 100%">OOP</span>
                    <span class="badge blue badge-element" style="font-size: 100%">Code documentation</span>
                    <span class="badge green badge-element" style="font-size: 100%">Material design</span>
                    <span class="badge orange badge-element" style="font-size: 100%">Multithreading</span>
                    <span class="badge grey badge-element" style="font-size: 100%">Intermediate english level</span>

                    <br>
                    <br>
                    <strong>Stackoverflow profile</strong>
                    <br>
                    <a href="https://stackexchange.com/users/9552681">
                        <img src="https://stackexchange.com/users/flair/9552681.png?theme=clean" width="208" height="58" alt="profile for Аксенов Владимир on Stack Exchange, a network of free, community-driven Q&amp;A sites" title="profile for Аксенов Владимир on Stack Exchange, a network of free, community-driven Q&amp;A sites">
                    </a>
                    <br>
                    </p>
                </div>
            </div>
        </div>
